{
  "name": "@nb/api",
  "private": true,
  "type": "module",
  "version": "0.1.0",
  "engines": {
    "node": ">=20 <21"
  },
  "scripts": {
    "dev": "node src/server.mjs",
    "prisma:generate": "prisma generate --schema ./prisma/schema.prisma",
    "prisma:migrate": "prisma migrate dev --name init --schema ./prisma/schema.prisma",
    "seed": "node scripts/seed.mjs",
    "export:dry": "node src/exporter/dry-run.mjs"
  },
  "dependencies": {
    "@prisma/client": "6.0.0",
    "cors": "2.8.5",
    "express": "4.19.2",
    "luxon": "3.4.4",
    "rrule": "2.8.1",
    "zod": "3.23.8"
  },
  "devDependencies": {
    "@types/cors": "2.8.17",
    "@types/express": "4.17.21",
    "@types/node": "20.14.12",
    "prisma": "6.0.0"
  }
}
